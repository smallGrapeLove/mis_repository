<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>mytag</short-name>
    <!-- 以下定义的uri用于在jsp页面中引入自定义标签库，需要特别注意一致。-->
    <uri>http://www.xuhuan.mis/tag</uri>
    <tag>
        <name>hello</name>   <!-- 标签在jsp中使用名称 -->
        <tag-class>com.xuhuan.mis.tag.TestTag</tag-class><!-- 标签指向的class文件 -->
        <body-content>JSP</body-content><!-- 标签内容为空 -->

        <attribute>
            <name>prefix</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <description>控件name</description>
        </attribute>

    </tag>
</taglib>